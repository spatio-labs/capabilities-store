{
  "type": "remote",
  "name": "Gmail Send Draft",
  "description": "Sends an existing draft email from the user's Gmail account.",
  "categories": [
    "Gmail"
  ],
  "tags": [
    "Gmail", "Email", "Draft", "Send"
  ],
  "entry_point": "main",
  "auth": {
    "type": "oauth2",
    "env_variable": "GOOGLE_AUTH_TOKEN"
  },
  "organization": "google",
  "group": "gmail",
  "inputs": [
    {
      "name": "draftId",
      "type": "string",
      "required": true,
      "description": "The ID of the draft to send"
    }
  ],
  "output": {
    "message": {
      "type": "object"
    }
  }
}
